Upload Image
<%= form_for @user, :url => users_path, :html => { :multipart => true } do |f| %>
<%= f.hidden_field :user_id, :value => @current_user.id %>
<div class="actions">
  <%= f.file_field :avatar %>
  <%= f.submit :submit, data: {disable_with: "Submitting..."} %>
<% end %>
<hr>
<%= link_to "Go Home", '/' %>